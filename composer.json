{
  "name": "yoast-seo-for-typo3/yoast_seo",
  "description": "Yoast SEO for TYPO3",
  "type": "typo3-cms-extension",
  "license": "GPL-3.0",
  "keywords": [
    "TYPO3 CMS",
    "Yoast",
    "SEO",
    "MaxServ"
  ],
  "authors": [
    {
      "name": "Team Yoast",
      "email": "support@yoast.com",
      "homepage": "https://yoast.com"
    },
    {
      "name": "Team MaxServ",
      "email": "support@maxserv.com",
      "homepage": "https://maxserv.com"
    }
  ],
  "homepage": "https://yoast.com",
  "require": {
    "typo3/cms-core": "^7.6 || ^8.7"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.0",
    "nimut/testing-framework": "^2.0"
  },
  "replace": {
    "yoast_seo": "self.version",
    "typo3-ter/yoast-seo": "self.version"
  },
  "autoload": {
    "psr-4": {
      "YoastSeoForTypo3\\YoastSeo\\": "Classes/"
    }
  },
  "config": {
    "vendor-dir": ".Build/vendor",
    "bin-dir": ".Build/bin"
  },
  "extra": {
    "typo3/cms": {
      "cms-package-dir": "{$vendor-dir}/typo3/cms",
      "web-dir": ".Build/Web"
    }
  }
}
